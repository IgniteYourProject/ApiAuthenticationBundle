{
  "name": "igniteyourproject/api-authentication",
  "type": "symfony-bundle",
  "description": "Stateless Token Based Api Authentication bundle.",
  "keywords": ["igniteyourproject", "stateless", "symfony", "bundle"],
  "homepage": "https://igniteyourproject.com",
  "license": "MIT",
  "require": {
    "php":                  ">=5.6",
    "symfony/symfony":      ">=2.7"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.1"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": { "IgniteYourProject\\ApiAuthenticationBundle\\": "" }
  },
  "autoload-dev": {
    "psr-4": { "IgniteYourProject\\ApiAuthenticationBundle\\spec\\": "spec/" }
  },
  "extra": {
    "installer-name": "IgniteYourProject/ApiAuthenticationBundle"
  }
}
